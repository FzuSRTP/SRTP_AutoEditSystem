/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 0;
  box-sizing: border-box;
} 
.tabBar_box
{
   display:flex;
   flex-direction:row;
   justify-content:space-around;
   position:fixed;
   bottom:0;
   width:100%;
   height:110rpx;
}
.tabBar_nav
{
  display:flex;
  flex-direction:column;
  justify-content:center;
  align-items:center;
  font-size:25rpx;
  height:100%;
}
.tabBar_icon
{
  width:55rpx;
  height:55rpx;
}
